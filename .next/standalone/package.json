{
  "name": "web-1",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "npm run lint",
    "development": "npx --yes dotenv-run-script ../.env.development --",
    "production": "npx --yes dotenv-run-script ../.env.production --",
    "db:pull": "npx --yes prisma db pull --schema=./src/prisma/schema.prisma",
    "db:push": "npx --yes prisma db push --accept-data-loss --force-reset --skip-generate --schema=./src/prisma/schema.prisma",
    "db:migrate:dev": "npx --yes prisma migrate dev --schema=./src/prisma/schema.prisma",
    "db:migrate:resolve": "npx --yes prisma migrate resolve --schema=./src/prisma/schema.prisma --applied",
    "db:migrate:dev:create": "npx --yes prisma migrate dev --create-only --schema=./src/prisma/schema.prisma",
    "db:migrate:prod": "npx --yes prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "db:gen": "npx --yes prisma generate --schema=./src/prisma/schema.prisma",
    "db:gen:watch": "npx --yes prisma generate --watch --schema=./src/prisma/schema.prisma",
    "db:reset": "npx --yes prisma migrate reset --schema=./src/prisma/schema.prisma",
    "prisma:format": "npx --yes prisma format --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@msalek/utils": "^2",
    "@prisma/client": "^6",
    "axios": "^1",
    "cloudinary": "^2.7.0",
    "cookies-next": "^5",
    "dayjs": "^1",
    "fast-geoip": "^1",
    "jsonwebtoken": "^9",
    "next": "^15",
    "react": "^18",
    "react-dom": "^18",
    "request-ip": "^3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9",
    "@types/node": "^20",
    "@types/react": "19.1.0",
    "@types/request-ip": "^0.0.41",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "eslint-config-prettier": "^9",
    "prisma": "^6",
    "typescript": "^5"
  }
}
